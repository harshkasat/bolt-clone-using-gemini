{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      }
    ]
}